
function appTarget(appName, callback){
  var locale = (typeof FDX.DATALAYER.page.pageInfo.locale !=="undefined") ? FDX.DATALAYER.page.pageInfo.locale : "en_us";
  var cCode = (typeof FDX.DATALAYER.page.pageInfo.country_code !=="undefined") ? FDX.DATALAYER.page.pageInfo.country_code : "us";
  var targetSegIds = (typeof _satellite.getVar('app:shipSegIds') !=="undefined") ? _satellite.getVar('app:shipSegIds') : "";
  
  //use data element  
  var targetUrl= "/shipping/shipEntryAction.do?method=doEntry&locale="+locale+"&urlparams="+cCode+"&sType=F";
  var defaultUrl="/lite/lite-ship.html?locale="+locale+"&cntry_code="+cCode+"#address";
  
  if(typeof appName !=="undefined" && appName.toLowerCase() == "ship"){
    
    var cookieDataList = FDX.SESSION.getCookie("aam_sc").replace(/\%7C/g,",").replace(/\aamsc%3D/g,"");
    var target = false;    
    
    if (targetSegIds !==""){
      var targetSegIdsList = targetSegIds.split(",");  // for multiple target segment IDs

      for ( var i =0; i< targetSegIdsList.length; i++){
         if(cookieDataList.indexOf(targetSegIdsList[i]) > -1){
            target = true;
            break;
          }   
      }
    }
    
    if(target){
      window.location.href = targetUrl;  
    }else{
    	window.location.href = defaultUrl;  
    }
    
  }
  
  return;
}
